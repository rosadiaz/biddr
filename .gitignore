!/biddr-rails-api/log/.keep
!/biddr-rails-api/tmp/.keep

# Ignore uploaded files in development
/biddr-rails-api/storage/*
!/biddr-rails-api/storage/.keep

.biddr-rails-api/byebug_history

# Ignore master key for decrypting credentials and more.
/biddr-rails-api/config/master.key


